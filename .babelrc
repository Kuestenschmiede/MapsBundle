{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react"
  ],
  "plugins": [
    "@babel/transform-react-jsx",
    "@babel/plugin-proposal-class-properties",
    "@babel/transform-modules-commonjs",
    "@babel/syntax-dynamic-import",
    "@babel/transform-runtime"
  ],
  "ignore": [
    "*/node_modules,*/"
  ]
}