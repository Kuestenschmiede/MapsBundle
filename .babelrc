{
  "presets": [
    "babel-preset-react",
    "es2015",
    "stage-0",
    [
      "env", {
        "modules": "commonjs"
      }
    ]
  ],
  "plugins": [
    "transform-react-jsx",
    "transform-class-properties",
    "transform-es2015-modules-commonjs",
    "syntax-dynamic-import",
    ["transform-runtime", {
      "regenerator": true
    }]
  ],
  "ignore": [
    "*/node_modules,*/"
  ]
}