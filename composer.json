{
  "name": "con4gis/maps",
  "type": "contao-bundle",
  "description": "The interactive maps brick of the Contao GIS-kit con4gis.",
  "keywords": ["con4gis", "maps", "openlayers", "osrm", "nominatim", "overpass","map", "gis", "osm", "openstreetmap", "bing", "mapbox", "contao", "c4g", "maps3", "here", "thunderforest", "MapsBundle"],
  "homepage": "https://con4gis.org",
  "license": "LGPL-3.0+",
  "authors": [
    {
      "name": "Kuestenschmiede GmbH Software & Design",
      "homepage": "https://kuestenschmiede.de",
      "role": "Operator"
    },
    {
      "name": "con4gis community",
      "homepage": "https://con4gis.org",
      "role": "Developer"
    }
  ],
  "support": {
    "email": "support@con4gis.org",
    "issues": "https://github.com/Kuestenschmiede/MapsBundle/issues"
  },
  "require": {
    "con4gis/core": "^2.0"
  },
  "conflict": {
    "contao/core": "*",
    "contao/manager-plugin": "<2.0 || >=3.0"
  },
  "autoload": {
    "psr-4": {
      "con4gis\\MapsBundle\\": ""
    },
    "classmap": [
      "Resources/contao/"
    ],
    "exclude-from-classmap": [
      "Resources/contao/config/",
      "Resources/contao/dca/",
      "Resources/contao/languages/",
      "Resources/contao/templates/"
    ]
  },
  "config": {
    "preferred-install": "dist"
  },
  "extra": {
    "contao-manager-plugin": "con4gis\\MapsBundle\\ContaoManager\\Plugin"
  }
}
