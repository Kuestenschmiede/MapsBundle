{
  "name": "con4gis/maps",
  "description": "Include interactive maps via OpenLayers (OpenStreetMap, Bing, Mapbox, ...)",
  "keywords": ["con4gis", "maps3", "openlayers", "map", "gis", "osm", "openstreetmap", "bing", "mapbox", "contao", "c4g"],
  "type": "contao-module",
  "homepage": "https://con4gis.org",
  "license": "LGPL-3.0+",
  "authors":[
    {
      "name":"KÃ¼stenschmiede GmbH Software & Design",
      "homepage":"https://kuestenschmiede.de"
    }
  ],
  "require": {
    "con4gis/core": "^2.0"
  },
  "conflict": {
    "contao/core": "*",
    "contao/manager-plugin": "<2.0 || >=3.0"
  },
  "support": {
    "issues": "https://github.com/Kuestenschmiede/MapsBundle/issues"
  },
  "autoload": {
    "psr-4": {
      "con4gis\\MapsBundle\\": ""
    },
    "classmap": [
      "Resources/contao/"
    ],
    "exclude-from-classmap": [
      "Resources/contao/config/",
      "Resources/contao/dca/",
      "Resources/contao/languages/",
      "Resources/contao/templates/"
    ]
  },
  "config": {
    "preferred-install": "dist"
  },
  "extra": {
    "contao-manager-plugin": "con4gis\\MapsBundle\\ContaoManager\\Plugin"
  }
}
